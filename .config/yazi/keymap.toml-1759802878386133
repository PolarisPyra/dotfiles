# Remove default 'gh' keybind
[[manager.prepend_keymap]]
on = ["g", "h"]
run = ""
desc = ""

# Your custom keybinds
[[manager.prepend_keymap]]
on = ["g", "s"]
run = 'cd ~/projects/'
desc = "Go to projects folder"

[[manager.prepend_keymap]]
on = ["g", "t"]
run = 'cd ~/temp'
desc = "Go to Temp"

[[manager.prepend_keymap]]
on = ["g", "c"]
run = 'cd ~/dotfiles'
desc = "Go to Dotfiles"

[[manager.prepend_keymap]]
on = ["g", "x"]
run = 'shell "xdg-open $@" --confirm --orphan'
desc = "Explorer here"

[[manager.prepend_keymap]]
on = ["<C-o>"]
run = 'back'
desc = "Back in history"

[[manager.prepend_keymap]]
on = ["<C-i>"]
run = 'forward'
desc = "Forward in history"